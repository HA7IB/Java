package com.company;

import java.util.Arrays;

public class Main{
    public static void main(String[] args){
        int[] numbers = {1,5,2,4,0,9};
        Arrays.sort(numbers);
        System.out.println(Arrays.toString(numbers));
    }
}



//eg 2(old system)

package com.company;

import java.lang.reflect.Array;
import java.util.Arrays;

public class Main{
    public static void main(String[] args){
        int[] numbers = new int[10];
        numbers[0]=1;
        numbers[1]=8;
        numbers[7]=7;
        System.out.println(Arrays.toString(numbers));
    }
}
